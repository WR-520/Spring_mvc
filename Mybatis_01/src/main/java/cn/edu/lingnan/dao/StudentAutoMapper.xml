<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.lingnan.dao.StudentAutoMapper">
    <insert id="insertStudent"  keyProperty="sid" useGeneratedKeys="true" parameterType="StudentAuto" >
      insert into school.studentauto(sname,password,superuser) values (#{sname},#{password},#{superuser});
    </insert>
</mapper>